#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
cd $DIR

LOG="`tempfile`"

if [ "$1" != "" ] && [ "$1" != "engine" ] && [ "$1" != "webui" ]; then
    echo "'$1' is not a part of Pipeline 2. Must be either 'engine' or 'webui'"
else
    ./pipeline.update.engine
    ./pipeline.update.webui
fi

exit 0

